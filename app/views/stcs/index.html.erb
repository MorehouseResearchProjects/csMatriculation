<p id="notice"><%= notice %></p>

<h1>Stcs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stcs.each do |stc| %>
      <tr>
        <td><%= link_to 'Show', stc %></td>
        <td><%= link_to 'Edit', edit_stc_path(stc) %></td>
        <td><%= link_to 'Destroy', stc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stc', new_stc_path %>
